  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8a8d92",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00aa66"
      }
    },
    {
      "name": "Template String",
      "scope": ["string.template"],
      "settings": {
        "foreground": "#00cc77"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#cc8800"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#cc0055",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#cc0055"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#cc0055",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cc00cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#cc0055"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00aacc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "support.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#cc8800",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#00aacc"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#2b2d31"
      }
    },
    {
      "name": "Variable Language (self, this)",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#cc00cc",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "entity.name.function.decorator", "punctuation.definition.decorator"],
      "settings": {
        "foreground": "#cc00cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#cc0055"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc0055"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cc8800"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#5a5d62"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cc0055",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#cc8800"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#00aacc"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#00aa66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#cc8800",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#cc00cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00aa66",
        "background": "#f5f6f760"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00aacc",
        "fontStyle": "underline"
      }
    }
  ]
}