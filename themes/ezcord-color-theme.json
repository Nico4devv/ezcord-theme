  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5a5d62",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Template String",
      "scope": ["string.template"],
      "settings": {
        "foreground": "#33ffaa"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ff0077",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#ff0077"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#ff0077",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff0077"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00ddff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "support.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#ffaa00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#00ddff"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#e3e5e8"
      }
    },
    {
      "name": "Variable Language (self, this)",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "entity.name.function.decorator", "punctuation.definition.decorator"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#ff0077"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff0077"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#8a8d92"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff0077",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#00ddff"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#00ff88",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffaa00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00ff88",
        "background": "#0a0b0d60"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00ddff",
        "fontStyle": "underline"
      }
    }
  ]
}