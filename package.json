{
  "name": "ezcord-theme",
  "displayName": "Ezcord Theme",
  "description": "A bold neon cyberpunk dark theme with vibrant colors and maximum contrast - designed to stand out from the crowd",
  "version": "1.1.1",
  "publisher": "zkawiy",
  "author": {
    "name": "kawi's Developing"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension/themeIndicator.js",
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "theme",
    "Icons"
  ],
  "galleryBanner": {
    "color": "#0d0e10",
    "theme": "dark"
  },
  "contributes": {
    "commands": [
      {
        "command": "ezcord.switchTheme",
        "title": "Ezcord: Switch Theme"
      },
      {
        "command": "ezcord.changeAccent",
        "title": "Ezcord: Change Accent Color"
      }
    ],
    "themes": [
      {
        "label": "Ezcord Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/ezcord-color-theme.json"
      },
      {
        "label": "Ezcord High Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/ezcord-highcontrast-theme.json"
      },
      {
        "label": "Ezcord Light",
        "uiTheme": "vs",
        "path": "./themes/ezcord-light-theme.json"
      },
      {
        "label": "Ezcord Pastel",
        "uiTheme": "vs-dark",
        "path": "./themes/ezcord-pastel-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "ezcord-icons",
        "label": "Ezcord Icons",
        "path": "./icons/ezcord-icon-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nico4devv/ezcord-theme"
  },
  "bugs": {
    "url": "https://github.com/Nico4devv/ezcord-theme/issues"
  },
  "icon": "extension/logo.png",
  "homepage": "https://github.com/Nico4devv/ezcord-theme#readme",
  "dependencies": {
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/node": "^25.0.3"
  }
}
